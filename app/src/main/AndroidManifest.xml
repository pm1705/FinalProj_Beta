<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalproj_beta">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProj_Beta"
        android:usesCleartextTraffic="true">
        <activity android:name=".my_requests"></activity>
        <activity android:name=".view_request" />
        <activity android:name=".pending_requests" />
        <activity android:name=".upload_file" />
        <activity android:name=".teacher_sign_in" />
        <activity android:name=".first_signin" />
        <activity android:name=".google_auth" />
        <activity android:name=".request_print" />
        <activity android:name=".home_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".send_request" />
        <activity android:name=".settings_page" />
    </application>

</manifest>